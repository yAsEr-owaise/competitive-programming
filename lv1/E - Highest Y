
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
#define ll long long ;
struct point{
int x,y;
};
bool cmp(point a, point b) {
    return a.y > b.y; 
}
void solve() {

int n;
cin>>n;
point arr[100];
for (int i = 0; i < n; i++)
{
     cin>>arr[i].x>>arr[i].y;
}
sort(arr,arr+n,cmp);
for (int i = 0; i < n; i++) {
        cout << arr[i].x << " " << arr[i].y << endl;
    }
    }
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
// int t;
  //cin >> t;
  //while(t--)
    solve();
}


