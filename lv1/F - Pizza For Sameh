typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
void solve() {
int n;
cin>>n;
int count=1;
for (int i = 1; i < 10000; i++)
{
   
  if(i*i+3*i*n-10*n*n==0)count++;
}
for (int i = -1; i >- 10000; i--)
{
  if(i*i+3*i*n-10*n*n==0)count++;
}
cout<<count<<endl;
cout<<0<<" ";
for (int i = 1; i < 10000; i++)
{
   
  if(i*i+3*i*n-10*n*n==0)cout<<i<<" ";
}
for (int i = -1; i >- 10000; i--)
{
  if(i*i+3*i*n-10*n*n==0)cout<<i<<" ";
}
}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
 // int t;
//cin >> t;
  //while(t--)
    solve();
}


