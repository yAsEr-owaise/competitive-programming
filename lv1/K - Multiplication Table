#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
     int n;
     cin>>n;
     for (int i = 1; i <=10; i++)
     {
         cout<<i<<" x "<<n<<" = "<<n*i<<endl;
     }
     
      }
 
