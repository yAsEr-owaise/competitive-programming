#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
  long long n;
  cin>>n;
  if(n>=13)cout<<"YES";
  else cout<<"NO";
 }
