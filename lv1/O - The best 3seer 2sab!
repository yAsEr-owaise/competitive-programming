
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
struct el{
   double v,l;
};
bool cmp(el a,el b){
   if((a.v/a.l)> (b.v/b.l))return true;
   else return false;
}
el arr[100000];
void solve() {
int n;
double L;
cin>>n>>L;

for (int i = 0; i < n; i++)
{
    cin>>arr[i].v>>arr[i].l;
}
sort(arr,arr+n,cmp);
double val=0;
for (int i = 0; i < n; i++)
{
   if(L<=0)break;
   if(L>=arr[i].l){
        val+=arr[i].v;
        L-=arr[i].l;
   }
   else {
      val+=arr[i].v*(L/arr[i].l);
      L=0;
   }
}
cout<<fixed<<setprecision(6)<<val;
}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  //int t;
 //cin >> t;
  //while(t--)
    solve();
}


