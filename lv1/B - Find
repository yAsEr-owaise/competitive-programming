#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
int arr[100000];
int n,x;
cin>>n>>x;
for (int i = 0; i < n; i++)
{
 cin>>arr[i];
}
if(find(arr,arr+n,x)!=arr+n)cout<<find(arr,arr+n,x)-arr;
else cout<<"Not Found";
 }
