#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
   int k,r;
   cin>>k>>r;
   int sh=1;
   long long price;
   while(true){
    price=sh*k;
    int price1=price%10;
    if(price%10==0 ||price1==r ){
      cout<<sh;
      break;
    }
    sh++;
   }
    }
