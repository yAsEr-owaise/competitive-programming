
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
const ll M = 1e9 + 7;
const ll N=2*1e5;
void solve() {
ll a, b, n, k;
    cin >> a >> b >> n >> k;
    if (k % a != 0) {
        cout << -1 << "\n";
        return;
    }
    ll val = k / a; 
    if (val > n || val < 0) {
        cout << -1 << "\n";
        return;
    }
    ll r = n - val;
    cout << r + 1 << "\n";
}

int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  int t;
cin >> t;
  while(t--)
    solve();
}


