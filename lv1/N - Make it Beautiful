
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
bool c (int arr[50],int a){
   for (int i = 0; i < a; i++)
   {
       if(arr[0]!=arr[i])return false;
   }
   return true;
}
bool pref(int arr[50],int a){
   int pref[51]={};
   for (int i = 1; i < a; i++)
   {
      pref[i]=arr[i]+pref[i-1];
   }
   for (int i = 1; i < a; i++)
   {
       if(arr[i]==pref[i])return false;
       
   }
   return true;
}
void solve() {
int arr[50];
int n;
cin>>n;
for (int i = 0; i < n; i++)
{
  cin>>arr[i];
}
if(c(arr,n))no;
else{
   if(pref(arr,n)){
      yes;
      for (int i = 0; i < n; i++)
      {
          cout<<arr[i]<<" ";
      }
   }
   else {
      yes;
      sort(arr,arr+n);
      swap(arr[1],arr[n-1]);
      for (int i = 0; i < n; i++)
      {
          cout<<arr[i]<<" ";
      }
   }
   cout<<endl;
}


   }
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  int t;
  cin >> t;
  while(t--)
    solve();
}


