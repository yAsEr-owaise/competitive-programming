
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
#define ll long long ;
struct star{
int x,y,z;
};
void solve() {
star arr[1000];
int n;
cin>>n;
star count;
count.x=0;
count.y=0;
count.z=0;
for (int i = 0; i < n; i++)
{
  cin>>arr[i].x>>arr[i].y>>arr[i].z;
}
for (int i = 0; i <  n; i++)
{
     for (int j = 0; j < n; j++)
     {
        if(i==j)continue;
        if(arr[i].x==arr[j].x)count.x++;
        if(arr[i].y==arr[j].y)count.y++;
        if(arr[i].z==arr[j].z)count.z++;
     }
     cout<<count.x<<" "<<count.y<<" "<<count.z<<endl;
     count.x=0;
count.y=0;
count.z=0;
}


}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
// int t;
  //cin >> t;
  //while(t--)
    solve();
}


