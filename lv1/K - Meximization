#include <bits/stdc++.h>
#include <ext/pb_ds/assoc_container.hpp> 
#include <ext/pb_ds/tree_policy.hpp>
#include<iomanip>
#include<numeric>
using namespace std;
using namespace __gnu_pbds;
typedef long long ll;
typedef tree<int, null_type, less<int>, rb_tree_tag, 
             tree_order_statistics_node_update> ordered_set;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.rbegin(), v.rend()
const ll M = 2e5;
const ll N=1e6;
void solve() {
    int n;
    cin>>n;
vector<int> v(n);
for(auto &c:v)cin>>c;
sort(all_vec(v));
deque<int> dq;
for(auto c:v){
    if(dq.empty()){
        dq.push_back(c);
        continue;
    }
    if(dq.back()==c)dq.push_front(c);
    else dq.push_back(c);
}
for (int i = n-1; i >=0; i--)
{
    cout<<dq[i]<<" ";
}
cout<<endl;
}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
 int t; cin >> t;
   while(t--)
     solve();
}
