
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
void solve() {
int arr[100];
int n;
cin>>n;
for (int i = 0; i < n; i++)
{
   cin>>arr[i];
}
int count=0,mx=-2;
for (int i = 0; i < n; i++)
{
  if(arr[i]==0)count++;
  else {
    mx=max(mx,count);
    count=0;
  }
}
mx=max(mx,count);

cout<<mx<<endl;

}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  int t;
cin >> t;
  while(t--)
    solve();
}


