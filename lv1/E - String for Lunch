
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
const ll M = 1e9 + 7;
void solve() {
string s;
cin>>s;
string k;
cin>>k;
sort(s.begin(),s.end());
sort(k.begin(),k.end());
for (int i = 0; i < s.len(); i++)
{
     if(s[i]!=k[i]){
        cout<<"no\n";
        return;
     }
}
cout<<"yes\n";
}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  int t;
cin >> t;
  while(t--)
    solve();
}


