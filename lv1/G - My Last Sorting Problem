
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
int m;
struct s{
  string s;
  int ind;
};
s arr[1000000];
bool cmp(s a,s b){
  for (int i = 0; i < m; i++){
    if (a.s[i] != b.s[i]){
            if (i % 2 == 0) { 
                return a.s[i] <= b.s[i]; 
            } else { 
                return a.s[i] >= b.s[i]; }
            }
          }
          }
void solve() {
int n;
cin>>n>>m;

for (int i = 0; i < n; i++)
{
   cin>>arr[i].s;
   arr[i].ind=i+1;
}
sort(arr,arr+n,cmp);
for (int i = 0; i < n; i++)
{
   cout<<arr[i].ind<<" ";
}

}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
 // int t;
//cin >> t;
  //while(t--)
    solve();
}


