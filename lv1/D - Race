#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
  int a,b,c,d;
  cin>>a>>b>>c>>d;
  int count=0;
  if(b>a)count++;
  if(c>a)count++;
  if(d>a)count++;
  cout<<count;
 }
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
cin >> t;
while(t--){
    solve();
cout<<endl;}

}
