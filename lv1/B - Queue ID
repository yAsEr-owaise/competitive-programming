#include <bits/stdc++.h>
#include <ext/pb_ds/assoc_container.hpp> 
#include <ext/pb_ds/tree_policy.hpp>
#include<iomanip>
#include<numeric>
using namespace std;
using namespace __gnu_pbds;
typedef long long ll;
typedef tree<int, null_type, less<int>, rb_tree_tag, 
             tree_order_statistics_node_update> ordered_set;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
const ll M = 2e5;
const ll N=1e6;
queue <int> q;
void solve() {
int id, n;
    cin >> id >> n;
    if (id == 1) {
        q.push(n);
    } 
    else if (id == 2) {
        if (q.empty()) {
            cout << "no" << endl;
        } else {
            if (q.front() == n) {
                cout << "yes" << endl;
            } else {
                cout << "no" << endl;
            }
            q.pop();
        }
    }
}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
 int t; cin >> t;
   while(t--)
     solve();
}
