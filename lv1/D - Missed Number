
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
typedef long long ll;
///\\\/// ||\/|| /\/\/\/\/\/
#define yes cout<<"YES\n"
#define no cout<<"NO\n"
#define len() length()
#define all_vec(v) v.begin(), v.end()
void solve() {
int arr[5]={};
int n=5;
for (int i = 0; i < n; i++)
{
   if(i==2)continue;
  cin>>arr[i];
}
arr[2]=arr[4]-arr[3];
int count=0,res=0;
for (int i = 0; i <3; i++)
{
   if(arr[i+2]-arr[i+1]==arr[i])count++;
}
arr[2]=arr[0]+arr[1];
for (int i = 0; i <3; i++)
{
   if(arr[i+2]-arr[i+1]==arr[i])res++;
}
cout<<max(res,count)<<endl;

}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);
   cout.tie(NULL);
  int t;
cin >> t;
  while(t--)
    solve();
}


