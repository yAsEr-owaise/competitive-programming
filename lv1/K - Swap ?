
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
int n;
cin>>n;
int arr[1000];
for(int i=0;i<n;i++)cin>>arr[i];
int mx=-100001,mn=100001;
for(int i=0;i<n;i++){
  mx=max(mx,arr[i]);
  mn=min(mn,arr[i]);
}
int a=find(arr,arr+n,mx)-arr;
int b=find(arr,arr+n,mn)-arr;
swap(arr[a],arr[b]);
for(int i=0;i<n;i++)cout<<arr[i]<<" ";
 }
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  //int t;
//cin >> t;
//while(t--){
    solve();
//cout<<endl;}

}
