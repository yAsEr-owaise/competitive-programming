using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
  int a,b;
  cin>>a>>b;
  if(a-b>=0)cout<<a-b;
  else cout<<"0";

  
 }
