#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}
 void solve(){
int arr[1000];
int n;
cin>>n;
for (int i = 0; i < n; i++)
{
 cin>>arr[i];
}
for (int i = 0; i < n; i++)
{
   if(arr[i]<=10)cout<<"A["<<i<<"] = "<<arr[i]<<"\n";
}

 }
