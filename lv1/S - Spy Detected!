
#include <bits/stdc++.h>
#include<iomanip>
#include<numeric>
using namespace std;
bool isprime(int x){
  if(x==2)return true;
  else{
    for (int i = 2; i < x; i++)
    {
     if(x%i==0)return false;

    }
    
  }
  return true;
}

void solve() {
int n;
cin>>n;
int arr[100];
for(int i=0;i<n;i++)cin>>arr[i];
for (int i = 0; i < n; i++)
{
  int c=count(arr,arr+n,arr[i]);
   if(c==1)cout<<i+1;
}

}
int main(){
  ios::sync_with_stdio(false);
  cin.tie(nullptr);

  int t;
cin >> t;
while(t--){
    solve();
cout<<endl;}

}
